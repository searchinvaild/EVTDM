FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1";
    object      nu_sludge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform 0.1;
    }
    rightWall
    {
        type            calculated;
        value           uniform 0.1;
    }
    lowerWall
    {
        type            calculated;
        value           uniform 0.1;
    }
    atmosphere
    {
        type            calculated;
        value           uniform 0.1;
    }
    defaultFaces
    {
        type            empty;
    }
}


